<ul>
  {{#each model as |task|}}
    <li>
      {{#bs-button type="default"}}
        {{#link-to 'tasks.show' task}}{{task.title}}{{/link-to}}
      {{/bs-button}}
    </li>
  {{else}}
    <li>
      {{#bs-alert type="warning"}}No one has created any tasks yet{{/bs-alert}}
    </li>
    {{/each}}
</ul>
